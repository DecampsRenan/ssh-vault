{
  "name": "ssh-vault",
  "version": "0.0.1",
  "description": "Securely manage your ssh keys",
  "bin": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "cli-app",
    "ssh",
    "ssh-keys",
    "keys"
  ],
  "scripts": {
    "dev": "pnpm link-cli && pnpm build --watch",
    "link-cli": "(pnpm uninstall --global ssh-vault || true) && pnpm link --global",
    "build-old": "esbuild ./src/index.ts --bundle --platform=node --target=node12 --outdir=dist",
    "build": "ncc build src/index.ts -o dist",
    "lint": "tsc --noEmit"
  },
  "author": {
    "name": "Renan Decamps",
    "email": "renan.decamps@gmail.com",
    "url": "https://github.com/decampsrenan"
  },
  "license": "MIT",
  "prettier": "@decampsrenan/prettier-config",
  "dependencies": {
    "@swan-io/boxed": "2.1.1",
    "chalk": "5.3.0",
    "commander": "12.0.0",
    "conf": "12.0.0",
    "ky": "1.2.3",
    "ora": "8.0.1",
    "prompts": "2.4.2",
    "ts-pattern": "5.1.1",
    "update-notifier": "7.0.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "12.0.1",
    "@decampsrenan/prettier-config": "3.0.0",
    "@types/node": "20.12.7",
    "@types/prompts": "2.4.9",
    "@types/update-notifier": "6.0.8",
    "@vercel/ncc": "0.38.1",
    "esbuild": "0.20.2",
    "eslint": "9.0.0",
    "eslint-plugin-unicorn": "52.0.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  }
}
